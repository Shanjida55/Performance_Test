              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import com.jayway.jsonpath.JsonPath;

// Get the response body
String response = prev.getResponseDataAsString();

// Extract bookingid from the response
def bookingId = JsonPath.read(response, &apos;$.bookingid&apos;);

// Convert bookingId to string
def bookingIdAsString = bookingId.toString();

// Log the extracted bookingId
log.info(&quot;Extracted bookingId: &quot; + bookingIdAsString);

// Store the bookingId as a string in a JMeter variable for future use
vars.put(&quot;bookingId&quot;, bookingIdAsString);

</stringProp>
